<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/SETTINGS/1.0.0" xsi:schemalocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <!-- Maven Central Deployment -->
            <id>ossrh</id>
            <username>${env.SONATYPE_USERNAME}</username>
            <password>${env.SONATYPE_PASSWORD}</password>
        </server>
        <server>
            <id>nexitia-snapshots</id>
            <username>${env.NX_NEXUS_USERNAME}</username>
            <password>${env.NX_NEXUS_PASSWORD}</password>
        </server>
         <server>
            <id>nexitia-releases</id>
            <username>${env.NX_NEXUS_USERNAME}</username>
            <password>${env.NX_NEXUS_PASSWORD}</password>
        </server>
        <server>
        	<id>jsoaggercore.repo</id>
            <username>${ENV_GIT_USERNAME}</username>
            <password>${ENV_GIT_PASSWORD}</password>
        </server>
    </servers>
</settings>
